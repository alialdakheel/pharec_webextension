{
  "manifest_version": 2,
  "name": "Pharec Alpha",
  "version": "0.1",

  "description": "Detect phishing pages.",

  "icons": {
    "48": "icons/pharec-48.png",
    "96": "icons/pharec-96.png"
  },

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["metadata.js"]
    }
  ]
}
